{
    "manifest_version": 2,
    "name": "Article Downloader v1.1",
    "version": "1.1",
    "description": "Downloads article in .txt & index info in .js",
    "icons": {
        "48": "icons/app-48.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/app-48.png",
        "default_title": "Article Downloader",
        "default_popup": "popup/choose_site.html"
    }
}